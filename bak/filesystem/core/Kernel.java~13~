{
  public static final byte ROOT = 1;
  public static final byte DIRECTORY = 2;
  public static final byte FILE = 3;


  public static final int ROOT_SIZE = 51; //in bytes
  public static final int DIRECTORY_SIZE = 37; //in bytes
  public static final int FILE_SIZE = 38; //in bytes

  public static final byte IS_DELETED = 1;

  public Kernel()
  {
  }

}
